import java.util.Scanner;
public class asalsayi {
    public static void main(String[] args) {
        Scanner inp = new Scanner(System.in);
        boolean asalMi=false;
        int sayi;
        System.out.println("1 - 100 arasındaki Asal Sayılar:");
        for (int i=2; i<=100;i++){
            if (sayi%i==0){
                asalMi=false;
            }else {
                asalMi=true;
            }
        }
    }
}
